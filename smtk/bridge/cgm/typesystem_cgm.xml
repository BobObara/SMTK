  <!-- Additional objects to be wrapped when building with CGM -->
  <namespace-type name="cgmsmtk">
    <namespace-type name="cgm">
      <enum-type name="CubitAttributeTypeExtensions">
        <include file-name="smtk/cgm/CAUUID.h" location="local"/>
      </enum-type>

      <object-type name="Bridge">
        <include file-name="smtk/cgm/Bridge.h" location="local"/>
      </object-type>

      <object-type name="ImportSolid">
        <include file-name="smtk/cgm/ImportSolid.h" location="local"/>
      </object-type>

      <object-type name="Engines">
        <include file-name="smtk/cgm/Engines.h" location="local"/>
      </object-type>

      <object-type name="ExportSolid">
        <include file-name="smtk/cgm/ExportSolid.h" location="local"/>
      </object-type>
    </namespace-type>
  </namespace-type>

  <value-type template="smtk::shared_ptr" args="smtk::bridge::cgmBridge">
  </value-type>

  <!-- Don't wrap methods that take pointers to CGM classes.
       Shiboken improperly scopes classnames in argument
       lists (e.g., it assumes smtk::model::Shell when ::Shell is
       specified), making it not only undesirable but impossible
       to wrap CGM pointers.
    -->
  <rejection class="smtk::bridge::cgmBridge" function-name="addBodyToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addVolumeUseToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addFaceUseToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addEdgeUseToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addVertexUseToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addShellToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addLoopToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addChainToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addVolumeToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addFaceToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addEdgeToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addVertexToManager"/>
  <rejection class="smtk::bridge::cgmBridge" function-name="addGroupToManager"/>

  @EXTRA_OBJECTS@

</typesystem>

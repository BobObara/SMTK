<?xml version="1.0"?>
<typesystem package="@TYPESYSTEM_NAME@">

  @EXTRA_TYPESYSTEMS@

  <!-- Ignore methods that refer directly to CGM pointers -->
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addGroupToManager', unmatched parameter type 'RefGroup*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addEdgeToManager', unmatched parameter type 'RefEdge*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addVertexUseToManager', unmatched parameter type 'CoVertex*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addCGMEntityToManager', unmatched parameter type 'GroupingEntity*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addCGMEntityToManager', unmatched parameter type 'RefEntity*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addBodyToManager', unmatched parameter type 'Body*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addChainToManager', unmatched parameter type 'Chain*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addEdgeUseToManager', unmatched parameter type 'CoEdge*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addVertexToManager', unmatched parameter type 'RefVertex*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addFaceToManager', unmatched parameter type 'RefFace*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addLoopToManager', unmatched parameter type 'Loop*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addVolumeToManager', unmatched parameter type 'RefVolume*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addCGMEntityToManager', unmatched parameter type 'SenseEntity*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addFaceUseToManager', unmatched parameter type 'CoFace*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addVolumeUseToManager', unmatched parameter type 'CoVolume*'"/>
  <suppress-warning text="skipping function 'cgmsmtk::cgm::Bridge::addShellToManager', unmatched parameter type 'Shell*'"/>

  <!-- ignore shared_from_this base class-->
  <!--
  <suppress-warning text="skipping field 'DescriptivePhrase::m_delegate' with unmatched type 'smtk::shared_ptr&lt;SubphraseGenerator&gt;'"/>
  <suppress-warning text="skipping field 'DescriptivePhrase::m_parent' with unmatched type 'smtk::shared_ptr&lt;smtk::model::DescriptivePhrase&gt;'"/>
  <suppress-warning text="skipping field 'DescriptivePhrase::m_subphrases' with unmatched type 'std::vector&lt;smtk::model::DescriptivePhrasePtr&gt;'"/>
  <suppress-warning text="skipping function 'smtk::model::DescriptivePhrase::findDelegate', unmatched return type 'smtk::model::SubphraseGeneratorPtr'"/>
  <suppress-warning text="skipping function 'smtk::model::DescriptivePhrase::parent', unmatched return type 'smtk::model::DescriptivePhrasePtr'"/>
  <suppress-warning text="skipping function 'smtk::model::DescriptivePhrase::setup', unmatched return type 'smtk::model::DescriptivePhrase::Ptr'"/>
  <suppress-warning text="skipping function 'smtk::model::DescriptivePhrase::subphrases', unmatched return type 'smtk::model::DescriptivePhrases&'"/>
  -->


  <!-- Additional objects to be wrapped when building with CGM -->
  <namespace-type name="cgmsmtk">
    <namespace-type name="cgm">
      <enum-type name="CubitAttributeTypeExtensions">
        <include file-name="smtk/cgm/CAUUID.h" location="local"/>
      </enum-type>

      <object-type name="Bridge">
        <include file-name="smtk/cgm/Bridge.h" location="local"/>
      </object-type>

      <object-type name="ImportSolid">
        <include file-name="smtk/cgm/ImportSolid.h" location="local"/>
      </object-type>

      <object-type name="Engines">
        <include file-name="smtk/cgm/Engines.h" location="local"/>
      </object-type>

      <object-type name="ExportSolid">
        <include file-name="smtk/cgm/ExportSolid.h" location="local"/>
      </object-type>
    </namespace-type>
  </namespace-type>

  <value-type template="smtk::shared_ptr" args="cgmsmtk::cgm::Bridge">
  </value-type>

  <!-- Don't wrap methods that take pointers to CGM classes.
       Shiboken improperly scopes classnames in argument
       lists (e.g., it assumes smtk::model::Shell when ::Shell is
       specified), making it not only undesirable but impossible
       to wrap CGM pointers.
    -->
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addBodyToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addVolumeUseToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addFaceUseToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addEdgeUseToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addVertexUseToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addShellToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addLoopToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addChainToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addVolumeToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addFaceToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addEdgeToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addVertexToManager"/>
  <rejection class="cgmsmtk::cgm::Bridge" function-name="addGroupToManager"/>

  @EXTRA_OBJECTS@

</typesystem>
